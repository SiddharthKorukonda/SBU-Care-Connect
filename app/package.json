{
  "name": "sbu-care-connect-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on tcp:5173 && npm run electron\"",
    "electron": "cross-env VITE_DEV=true electron ./electron/main.js",
    "build": "vite build",
    "preview": "vite preview",
    "dist": "electron-builder"
  },
  "dependencies": {
    "framer-motion": "^11.2.10",
    "leaflet": "^1.9.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.0.2",
    "electron-builder": "^25.1.8",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "vite": "^5.4.10",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "org.sbu.careconnect",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "build"
    }
  }
}